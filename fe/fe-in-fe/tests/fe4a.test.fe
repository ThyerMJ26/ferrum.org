, [ ["name", "func-curried"]
  , ["language", "ferrum/0.1"]
  , ["primitives", "../fe/primitives/vso.fe"]
  , ["decls",
    """
       let f = 
           (a : Int) -> (b : Int) -> (c : Int) -> 
           (a + c) * (b - c);
       let x = f 3 7 1;
       let g = (a : Int) ->
           let h = 
               (b : Int) -> (c : Int) -> 
               (a + c) * (b - c);
           h;
       let y = g 3 7 1;
    """
    ]
  , ["expectType",  "f", "{ Int -> Int -> Int -> Int }"]
  , ["expectValue", "x", "24"]
  , ["expectType",  "x", "Int"]
  , ["expectType",  "g", "{ Int -> Int -> Int -> Int }"]
  , ["expectValue", "y", "24"]
  , ["expectType",  "y", "Int"]
  ]

